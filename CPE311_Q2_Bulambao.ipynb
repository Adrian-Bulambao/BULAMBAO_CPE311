    "def sequence(sq):\n",
    "    a = []\n",
    "    b = []\n",
    "    for i in range(len(sq)-1):\n",
    "        a.append(sq[i]+sq[i+1])\n",
    "        b.append([sq[i],sq[i+1]])\n",
    "    c = a.index(max(a))\n",
    "    return b[c]\n",
    "seq = [3,4,6,5,7,8,9]\n",
    "print(sequence(seq))"
   ]
